{
  "expo": {
    "name": "murai-mobile",
    "slug": "murai-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "muraimobile",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.murai.mobile"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.murai.mobile"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "fonts": [
      {
        "asset": "./assets/fonts/Poppins-Thin.ttf",
        "family": "Poppins-Thin"
      },
      {
        "asset": "./assets/fonts/Poppins-ThinItalic.ttf",
        "family": "Poppins-ThinItalic"
      },
      {
        "asset": "./assets/fonts/Poppins-ExtraLight.ttf",
        "family": "Poppins-ExtraLight"
      },
      {
        "asset": "./assets/fonts/Poppins-ExtraLightItalic.ttf",
        "family": "Poppins-ExtraLightItalic"
      },
      {
        "asset": "./assets/fonts/Poppins-Light.ttf",
        "family": "Poppins-Light"
      },
      {
        "asset": "./assets/fonts/Poppins-LightItalic.ttf",
        "family": "Poppins-LightItalic"
      },
      {
        "asset": "./assets/fonts/Poppins-Regular.ttf",
        "family": "Poppins-Regular"
      },
      {
        "asset": "./assets/fonts/Poppins-Italic.ttf",
        "family": "Poppins-Italic"
      },
      {
        "asset": "./assets/fonts/Poppins-Medium.ttf",
        "family": "Poppins-Medium"
      },
      {
        "asset": "./assets/fonts/Poppins-MediumItalic.ttf",
        "family": "Poppins-MediumItalic"
      },
      {
        "asset": "./assets/fonts/Poppins-SemiBold.ttf",
        "family": "Poppins-SemiBold"
      },
      {
        "asset": "./assets/fonts/Poppins-SemiBoldItalic.ttf",
        "family": "Poppins-SemiBoldItalic"
      },
      {
        "asset": "./assets/fonts/Poppins-Bold.ttf",
        "family": "Poppins-Bold"
      },
      {
        "asset": "./assets/fonts/Poppins-BoldItalic.ttf",
        "family": "Poppins-BoldItalic"
      },
      {
        "asset": "./assets/fonts/Poppins-ExtraBold.ttf",
        "family": "Poppins-ExtraBold"
      },
      {
        "asset": "./assets/fonts/Poppins-ExtraBoldItalic.ttf",
        "family": "Poppins-ExtraBoldItalic"
      },
      {
        "asset": "./assets/fonts/Poppins-Black.ttf",
        "family": "Poppins-Black"
      },
      {
        "asset": "./assets/fonts/Poppins-BlackItalic.ttf",
        "family": "Poppins-BlackItalic"
      }
    ]
  }
}
